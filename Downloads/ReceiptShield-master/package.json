{
  "name": "receipt-shield",
  "version": "0.1.0",
  "description": "AI-Powered Receipt Management & Fraud Detection System",
  "author": "ReceiptShield Team",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ReceiptShield.git"
  },
  "keywords": [
    "receipt-management",
    "fraud-detection",
    "ai",
    "nextjs",
    "firebase",
    "expense-tracking"
  ],
  "scripts": {
    "dev": "next dev --turbopack --port 9003 --hostname 0.0.0.0",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "build:production": "next build && next export",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 1000",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "echo \"Testing setup required: npm install --save-dev jest @testing-library/react @testing-library/jest-dom @testing-library/user-event @types/jest jest-environment-jsdom ts-jest\"",
    "test:watch": "echo \"Testing setup required\"",
    "test:coverage": "echo \"Testing setup required\"",
    "test:unit": "echo \"Testing setup required\"",
    "test:integration": "echo \"Testing setup required\"",
    "test:e2e": "echo \"Testing setup required\"",
    "test:ci": "echo \"Testing setup required\"",
    "extract-dataset": "tsx ml/extract_dataset.ts",
    "firebase:emulators": "firebase emulators:start",
    "firebase:deploy": "firebase deploy",
    "firebase:deploy:rules": "firebase deploy --only firestore:rules,storage",
    "firebase:deploy:apphosting": "firebase deploy --only apphosting",
    "deploy:production": "npm run build && firebase deploy --only apphosting",
    "deploy:all": "npm run build && firebase deploy",
    "analyze": "ANALYZE=true npm run build",
    "build:analyze": "cross-env ANALYZE=true npm run build"
  },
  "dependencies": {
    "@genkit-ai/googleai": "^1.8.0",
    "@genkit-ai/next": "^1.8.0",
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@sendgrid/mail": "^8.1.6",
    "@tanstack-query-firebase/react": "^1.0.5",
    "@tanstack/react-query": "^5.66.0",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-writer": "^1.6.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "firebase": "^11.10.0",
    "genkit": "^1.8.0",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.475.0",
    "next": "^15.5.2",
    "nodemailer": "^7.0.9",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@next/eslint-plugin-next": "^15.5.4",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jspdf": "^2.0.0",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint-config-prettier": "^9.1.0",
    "firebase-tools": "^14.18.0",
    "genkit-cli": "^1.8.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.1",
    "tesseract.js": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
